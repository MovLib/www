{
  "hostname": "movlib.org",
  "version": "0.0.1",
  "release": "dev",
  "locale": "en_US",
  "timezone": "Etc/UTC",
  "webmaster": "webmaster@movlib.org",
  "user": "movdev",
  "group": "www-data",
  "directory": {
    "bin": "/usr/local/bin",
    "cache": "/var/cache",
    "etc": "/etc",
    "lib": "/var/lib",
    "log": "/var/log",
    "run": "/run",
    "src": "/usr/local/src",
    "tmp": "/tmp"
  },
  "elasticsearch": {
    "version": "1.0.1"
  },
  "java": {
    "version": 7,
    "release": "trusty"
  },
  "mariadb": {

  },
  "nginx": {
    "configure": {
      "arguments": [
        "user={{ user }}",
        "group={{ group }}",
        "prefix=/usr/local",
        "sbin-path=/usr/local/sbin",
        "conf-path=/etc/nginx/nginx.conf",
        "pid-path=/run/nginx/nginx.pid",
        "lock-path=/var/lock/nginx.lock",
        "http-client-body-temp-path=/tmp",
        "http-fastcgi-temp-path=/tmp",
        "http-log-path=/var/log/nginx/access.log",
        "with-cc-opt='-O3 -m64 -march=native -ffunction-sections -fdata-sections -D FD_SETSIZE=131072'",
        "with-ld-opt='-Wl,--gc-sections'",
        "with-ipv6",
        "with-http_ssl_module",
        "with-http_spdy_module",
        "with-openssl-opt='enable-ec_nistp_64_gcc_128 no-rc2 no-rc4 no-rc5 no-md2 no-md4 no-ssl2 no-ssl3 no-krb5 no-hw no-engines'",
        "with-openssl={{ openssl }}",
        "with-md5={{ openssl }}",
        "with-md5-asm",
        "with-sha1={{ openssl }}",
        "with-sha1-asm",
        "with-pcre={{ pcre }}",
        "with-pcre-jit",
        "with-zlib={{ zlib }}",
        "without-http_access_module",
        "without-http_auth_basic_module",
        "without-http_autoindex_module",
        "without-http_empty_gif_module",
        "without-http_geo_module",
        "without-http_limit_conn_module",
        "without-http_limit_req_module",
        "without-http_memcached_module",
        "without-http_proxy_module",
        "without-http_referer_module",
        "without-http_scgi_module",
        "without-http_split_clients_module",
        "without-http_ssi_module",
        "without-http_upstream_ip_hash_module",
        "without-http_userid_module",
        "without-http_uwsgi_module"
      ]
    },
    "modules": [
      "git://github.com/Fleshgrinder/nginx_accept_language_module.git"
    ],
    "opensslVersion": "1.0.1f",
    "pcreVersion": "8.34",
    "version": "1.5.11"
  },
  "nodejs": {
    "npm": [ "autoprefixer", "bower", "csso", "node-zopfli", "recess", "svgo" ]
  },
  "php": {
    "checksum": "e760656f7cf2f05158f73da75e8b720b",
    "version": "5.5.10"
  }
}
