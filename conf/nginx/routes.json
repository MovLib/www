[
  {
    "route": "= /",
    "presenter": "Home"
  },
  {
    "route": "= /main.php",
    "return": "301 /"
  },
  {
    "route": "= /__(movies)",
    "presenter": "Movies"
  },
  {
    "route": "~/__(movies)/([0-9]+)$",
    "return": "301 /__(movie)/$1"
  },
  {
    "prefix": "= /__(movie)",
    "return": "301 /"
  },
  {
    "route": "~ /__(movie)/([0-9]+)$",
    "presenter": "Movie",
    "vars": {
      "movie_id": "$1"
    }
  }
]
